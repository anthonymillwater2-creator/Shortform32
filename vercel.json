{
  "redirects": [
    {
      "source": "/donations",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/donations.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ]
}
